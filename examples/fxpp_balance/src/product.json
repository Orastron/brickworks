{
	"product": {
		"name":	"Brickworks stereo balance example (C++)",
		"version": "1.1.0",
		"buildVersion": "1",
		"bundleName": "bw_example_fxpp_balance",
		"buses": [
			{
				"type":	"audio",
				"direction": "input",
				"channels": "stereo",
