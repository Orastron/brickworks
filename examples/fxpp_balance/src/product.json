				"maximum": 0.0,
				"unit": "db",
				"map": "linear"
			},
			{
				"name": "Right level",
				"shortName": "R level",
				"id": "r_level",
				"direction": "output",
				"defaultValue": -60.0,
				"minimum": -60.0,
				"maximum": 0.0,
				"unit": "db",
				"map": "linear"
			}
